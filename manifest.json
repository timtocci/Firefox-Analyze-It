{

    "manifest_version": 2,
    "id": "jid1-wpeLy0aB4veaxA@jetpack",
    "name": "Analyze-It",
    "description": "Opens various web based page and domain tools in a new tab",
    "version": "1.0.0",
    "author": "Timothy Tocci",
    "homepage_url": "https://addons.mozilla.org/en-US/firefox/addon/analyze-it/",
    "applications": {
        "gecko": {
            "strict_min_version": "56.0a1"
        }
    },

    "background": {
        "scripts": ["background.js"]
    },

    "permissions": [
        "menus",
        "activeTab"
    ],

    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png"
    }
}